{
  "name": "portfolio-starter-inesfrederickx-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "test-watch": "jest --watchAll --detectOpenHandles",
    "test": "jest --detectOpenHandles",
    "go": "npx knex migrate:latest --cwd ./src/db && nodemon src/index.js",
    "compose:build": "docker-compose down && docker-compose up --build && nodemon src/index.js",
